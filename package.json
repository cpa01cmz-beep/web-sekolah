{
  "name": "vite-cf-do-v2-runner",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist .wrangler .cache tsconfig.tsbuildinfo",
    "dev": "vite --host 0.0.0.0 --port ${PORT:-3000}",
    "build": "vite build",
    "lint": "eslint --cache -f json --quiet .",
    "lint:fix": "eslint --cache --fix .",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"worker/**/*.{ts,json,md}\" \"shared/**/*.{ts,json,md}\"",
    "format:src": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:worker": "prettier --write \"worker/**/*.{ts,json,md}\"",
    "format:shared": "prettier --write \"shared/**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"worker/**/*.{ts,json,md}\" \"shared/**/*.{ts,json,md}\"",
    "typecheck": "tsc --noEmit",
    "preview": "bun run build && vite preview --host 0.0.0.0 --port ${PORT:-4173}",
    "deploy": "npm run build && wrangler deploy",
    "deploy:staging": "npm run build && wrangler deploy --env staging",
    "deploy:production": "npm run build && wrangler deploy --env production",
    "health": "./scripts/health-check.sh staging",
    "health:staging": "./scripts/health-check.sh staging",
    "health:production": "./scripts/health-check.sh production",
    "health:json": "./scripts/health-check.sh staging --json",
    "status": "./scripts/deployment-status.sh staging",
    "status:staging": "./scripts/deployment-status.sh staging",
    "status:production": "./scripts/deployment-status.sh production",
    "status:json": "./scripts/deployment-status.sh staging --json",
    "predeploy": "./scripts/pre-deploy-check.sh staging",
    "predeploy:staging": "./scripts/pre-deploy-check.sh staging",
    "predeploy:production": "./scripts/pre-deploy-check.sh production",
    "rollback": "./scripts/rollback.sh",
    "rollback:staging": "./scripts/rollback.sh staging",
    "rollback:production": "./scripts/rollback.sh production",
    "cf-typegen": "wrangler types",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:changed": "vitest run --changed",
    "validate": "npm run typecheck && npm run lint && npm run test:run",
    "check": "npm run typecheck && npm run lint",
    "validate:env": "./scripts/validate-env.sh staging",
    "validate:env:staging": "./scripts/validate-env.sh staging",
    "validate:env:production": "./scripts/validate-env.sh production",
    "validate:env:json": "./scripts/validate-env.sh staging --json",
    "metrics": "./scripts/deployment-metrics.sh staging",
    "metrics:staging": "./scripts/deployment-metrics.sh staging",
    "metrics:production": "./scripts/deployment-metrics.sh production",
    "metrics:json": "./scripts/deployment-metrics.sh staging --json",
    "report": "./scripts/deployment-report.sh staging",
    "report:staging": "./scripts/deployment-report.sh staging",
    "report:production": "./scripts/deployment-report.sh production",
    "report:json": "./scripts/deployment-report.sh staging --json",
    "ci:ready": "./scripts/ci-readiness.sh",
    "ci:ready:json": "./scripts/ci-readiness.sh --json",
    "ci:ready:strict": "./scripts/ci-readiness.sh --strict",
    "config:validate": "./scripts/deployment-config-validator.sh",
    "config:validate:json": "./scripts/deployment-config-validator.sh --json",
    "config:validate:strict": "./scripts/deployment-config-validator.sh --strict",
    "vercel:build": "vite build",
    "vercel:preview": "vite preview --host 0.0.0.0 --port ${PORT:-4173}"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.90.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "hono": "^4.11.5",
    "immer": "^11.1.3",
    "jose": "^6.1.3",
    "lucide-react": "^0.575.0",
    "pino": "^10.2.1",
    "pino-pretty": "^13.1.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.71.1",
    "react-router-dom": "^7.12.0",
    "recharts": "^3.6.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.3.6",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.21.2",
    "@cloudflare/workers-types": "^4.20260227.0",
    "@eslint/js": "^9.39.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.10",
    "@types/pino": "^7.0.4",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.31.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^17.0.0",
    "happy-dom": "^20.3.7",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "3.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "wrangler": "^4.60.0"
  }
}
